{
  "name": "alexandria-adslot",
  "description": "A UI component library. By Adslot",
  "version": "0.5.0",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/alexandria.git"
  },
  "main": "dist/alexandria-main.js",
  "scripts": {
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "codecov": "cat coverage/lcov.info | codecov",
    "posttest": "npm run lint && npm run sass-lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "webpack --env=dist",
    "profile": "webpack --env=dist --profile --json > stats.json",
    "lint": "eslint ./src ./test",
    "sass-lint": "sass-lint './src/**/*.scss' -v",
    "clean": "rimraf dist/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "author": "Adslot",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-strict-mode": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.13.1",
    "file-loader": "^0.8.5",
    "generator-react-webpack": "^3.2.1",
    "isparta-loader": "^2.0.0",
    "jscs": "^2.7.0",
    "karma": "^0.13.16",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.2",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "object-assign": "^4.0.1",
    "open": "^0.0.5",
    "phantomjs": "^1.9.19",
    "react-addons-test-utils": "^0.14.5",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.0",
    "sass-lint": "^1.4.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.14.5",
    "react-dom": "^0.14.5"
  }
}
