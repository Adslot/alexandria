{
  "name": "alexandria-adslot",
  "description": "A UI component library. By Adslot",
  "version": "2.4.0",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/alexandria.git"
  },
  "main": "dist/alexandria-main.js",
  "scripts": {
    "start": "node server.js --env=dev",
    "start:cold": "node server.js --env=dev-cold",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "codecov": "cat coverage/coverage-final.json | codecov",
    "posttest": "npm run lint && npm run sass-lint && npm run jscs-lint && npm run check-commit",
    "dist": "webpack --bail --env=dist",
    "profile": "webpack --env=dist --profile --json > stats.json",
    "lint": "eslint ./src --rulesdir ./src && eslint ./test --rulesdir ./test",
    "sass-lint": "sass-lint './src/**/*.scss' -v",
    "jscs-lint": "jscs ./src ./test",
    "jscs-fix": "jscs ./src ./test --fix",
    "clean": "rimraf dist/*",
    "check-commit": "./scripts/commit-msg",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "author": "Adslot",
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-strict-mode": "^6.6.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-lodash": "^1.6.5",
    "eslint-plugin-react": "^5.0.1",
    "file-loader": "^0.8.5",
    "generator-react-webpack": "^3.2.2",
    "isparta-loader": "^2.0.0",
    "jscs": "^2.10.1",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "object-assign": "^4.0.1",
    "open": "^0.0.5",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.8.2",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "sass-lint": "=1.5.1",
    "sass-loader": "^3.1.2",
    "semver": "^5.1.0",
    "shelljs": "^0.6.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "lodash": "^4.10.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "svg4everybody": "^2.0.3"
  }
}
