{
  "name": "alexandria-adslot",
  "description": "A UI component library. By Adslot",
  "version": "4.0.2",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/alexandria.git"
  },
  "main": "dist/alexandria-main.js",
  "scripts": {
    "check-commit": "node scripts/commit-msg",
    "clean": "rimraf dist/*",
    "codecov": "cat coverage/coverage-final.json | codecov",
    "dist": "webpack --bail --env=dist",
    "jscs-lint": "jscs ./src ./test",
    "jscs-fix": "jscs ./src ./test --fix",
    "eslint": "eslint --ext .jsx,.js --rulesdir ./src ./src && eslint  --ext .jsx,.js --rulesdir ./test ./test",
    "lint": "npm run eslint && npm run sass-lint && npm run jscs-lint && npm run check-commit",
    "posttest": "npm run lint",
    "profile": "webpack --env=dist --profile --json > stats.json",
    "release:major": "git checkout master && git pull && npm version major && npm publish && git push --follow-tags",
    "release:minor": "git checkout master && git pull && npm version minor && npm publish && git push --follow-tags",
    "release:patch": "git checkout master && git pull && npm version patch && npm publish && git push --follow-tags",
    "sass-lint": "sass-lint './src/**/*.scss' -v",
    "start": "node server.js --env=dev",
    "start:cold": "node server.js --env=dev-cold",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "author": "Adslot",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-lodash": "^1.10.3",
    "eslint-plugin-react": "=6.5.0",
    "file-loader": "^0.9.0",
    "generator-react-webpack": "^3.3.4",
    "isparta-loader": "^2.0.0",
    "jscs": "^3.0.7",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-webpack": "^1.8.0",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.13.0",
    "null-loader": "^0.1.1",
    "object-assign": "^4.1.0",
    "open": "^0.0.5",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "=15.3.2",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.0.2",
    "semver": "^5.3.0",
    "shelljs": "^0.7.5",
    "sinon": "^2.0.0-pre.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "lodash": "=3.10.1",
    "react": "=15.3.2",
    "react-dom": "=15.3.2",
    "svg4everybody": "^2.1.3"
  },
  "engines": {
    "node": "^4.6"
  }
}
