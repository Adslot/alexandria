{
  "name": "alexandria-adslot",
  "private": false,
  "version": "0.3.4",
  "description": "A UI component library. By Adslot",
  "main": "dist/alexandria-main.js",
  "scripts": {
    "start": "node server.js --env=dev",
    "test": "karma start",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "webpack --env=dist",
    "profile": "webpack --env=dist --profile --json > stats.json",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "clean": "rimraf dist/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "repository": "",
  "keywords": [
    "UI Library",
    "components"
  ],
  "author": "Adslot",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-strict-mode": "^6.0.15",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "chai": "^3.4.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.21.0",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.6.3",
    "file-loader": "^0.8.4",
    "generator-react-webpack": "^2.2.7",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jscs": "^2.5.0",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.1",
    "null-loader": "^0.1.1",
    "object-assign": "^4.0.1",
    "open": "^0.0.5",
    "phantomjs": "^1.9.18",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  }
}
